{
  "manifest_version": 2,
  "name": "DuckDuckGo !bangs but Faster",
  "version": "0.2.2",
  "description": "Processes DuckDuckGo !bangs client-side",
  "icons": {
    "16": "icons/icon-16.png",
    "24": "icons/icon-24.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://duckduckgo.com/*",
    "*://bangs-but-faster.invalid/*",
    "https://duckduckgo.com/bang.js"
  ],
  "background": {
    "scripts": [
      "browser-polyfill.min.js",
      "background.js"
    ]
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "DDG !Bangs",
      "keyword": "!",
      "favicon_url": "https://gitlab.com/Inclushe/duckduckgo-bangs-but-faster/-/raw/0454ac1034d5d1b31cd70dd474448d69bfc72802/icons/icon-16.png?inline=false",
      "search_url": "https://bangs-but-faster.invalid/?q={searchTerms}",
      "encoding": "UTF-8",
      "is_default": false
    }
  }
}
